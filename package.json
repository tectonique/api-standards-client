{
  "name": "@tectonique/api-standards-client",
  "repository": "https://github.com/tectonique/api-standards-client",
  "description": "Client side implementation for API Standards (response envelopes and Problem Details).",
  "keywords": [
    "axios",
    "fetch",
    "typesafety",
    "envelope",
    "api",
    "problemdetails",
    "rfc7807",
    "7807"
  ],
  "author": {
    "name": "Peter Kuhmann",
    "email": "peter@hedgehogs-mind.com"
  },
  "version": "0.0.3",
  "publishConfig": {
    "access": "public"
  },
  "types": "dist/mjs/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "/dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./fixup.sh",
    "dev": "tsx demo.ts"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "axios": "^1.3.2",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "tsx": "^3.12.2",
    "typescript": "*"
  },
  "dependencies": {
    "@tectonique/api-standards": "^0.0.13"
  },
  "peerDependencies": {
    "axios": ">= 1.0.0"
  },
  "peerDependenciesMeta": {
    "axios": {
      "optional": false
    }
  }
}
